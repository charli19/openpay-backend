<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd"
	logicalFilePath="db.changelog-property-definition.xml">

	<!-- Definition of types to be used in all changesets -->
	
	<property name="ID_TYPE" value="BIGINT" />
	<property name="CODE_TYPE" value="VARCHAR(255)" />
	<property name="NAME_TYPE" value="VARCHAR(50)" />
	<property name="SURNAME_TYPE" value="VARCHAR(150)" />
	<property name="COIN_TYPE" value="BIGINT" />
	<property name="DESCRIPTION_TYPE" value="VARCHAR(250)" />
	<property name="DOCUMENT_NUMBER_TYPE" value="VARCHAR(9)" />
	<property name="AUDITABLE_DATE_TYPE" value="DATETIME2" />
	<property name="AUDITABLE_USER_TYPE" value="VARCHAR(255)" />

</databaseChangeLog>